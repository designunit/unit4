{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rulesDirectory": [
    ],
    "rules": {
        "no-console": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ]
    }
}